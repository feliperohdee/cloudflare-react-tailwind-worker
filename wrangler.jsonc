{
	"$schema": "node_modules/wrangler/config-schema.json",
	"compatibility_date": "2025-03-10",
	"compatibility_flags": ["nodejs_compat"],
	"main": "worker/index.tsx",
	"name": "react-tailwind-worker",
	"assets": {
		"binding": "ASSETS",
		"directory": "./public"
	},
	"observability": {
		"enabled": true
	},
	"vars": {
		"PRODUCTION": "true"
	}
}
